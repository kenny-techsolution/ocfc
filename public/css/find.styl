#find-on-map
	min-width 1100px
	display block
	position relative
	height 100%
	width 100%
	&>.container
		display block
		position absolute
		width 50%
		min-width 550px
		height 100%
		top 0px
		left 0px
		padding-top 70px

.summary-section
	width 510px
	height 45%
	border 1px solid red
	bottom 0px
	display block
	float right
	
.search-section
	width 510px
	height 55%
	float right
	.btn-default
		height 34px
		border-left 2px solid #C5C5C5 !important
	.choose-type
		width 100px
	.search-box
		width 306px

.search-section__search-bar
	
	
.search-section__autocomplete-result
	color black
	position absolute
	top 50px
	

.search-section__geo-code
	color black

.search-section__fellowships-result
	color black
	overflow-y scroll
	height 206px


#map-container
	position absolute
	top:0
	bottom 0
	left 0
	right 0
	margin 0
	padding 0

#find-map
	display block
	position absolute
	width 50%
	min-width 550px
	height calc(100% - 50px)
	top 50px
	right 0px
	